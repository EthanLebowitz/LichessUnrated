{
	"manifest_version": 2,
	"name": "Rating Free Lichess",
	"version": "1.0",
	
	"permissions": [
		"storage"
	],
	
	"icons": { "16": "icons/icon16active.png",
           "48": "icons/icon48active.png",
          "128": "icons/icon128active.png" },

	"content_scripts": [
		{
			"matches": ["https://lichess.org/*"],
			"js": ["content.js"],
			"css": ["styleb.css"]
		}
	],
	"browser_action": {},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}